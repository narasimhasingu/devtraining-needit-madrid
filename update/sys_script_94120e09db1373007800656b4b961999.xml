        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Add your code here
checkStatus();
	function checkStatus(){
		//get all rules using GlideRecord and add Query
		var rule = new GlideRecord('x_58872_needit_airlines_rule');
		rule.addNotNullQuery('condition');
		rule.orderBy('order');
		rule.query();
		if(rule.next()){
			var match = new GlideFilter.checkRecord(current,rule.condition,true);
			gs.print('>>>DEBUG: rule'+rule.status+'match='+match);
			if(match){
				current.status=rule.status;
				gs.addInfoMessage(gs.getMessage('airline_status_upgrade',[current.customer.getDisplayValue(),rule.status]));
			}
		}
	}
	
})(current, previous);]]></script>
